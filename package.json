{
	"name": "summersend",
	"version": "1.0.0",
	"main": "dist/electron.js",
	"directories": {
		"lib": "lib"
	},
	"scripts": {
		"compile-and-run": "npm run build-and-pack && npm run start",
		"start": "electron .",
		"compile-watch": "tsc --watch --pretty",
		"build-and-pack": "npm run compile && npm run webpack",
		"build-electron": "npm run compile-electron && npm run pack-and-electron",
		"pack-and-electron": "npm run webpack && electron .",
		"compile": "tsc --p ./",
		"compile-test": "npm run compile && npm run test",
		"compile-electron": "tsc --p ./src-electron",
		"webpack": "webpack ./src --output-path dist",
		"test": "",
		"test-server": "ws",
		"doc-build": "npm run compile && jsdoc ./src -r -c jsdoc.config.json --verbose"
	},
	"author": "Naka",
	"license": "GNU General Public License v3.0",
	"description": "",
	"dependencies": {
		"@types/jest": "^24.0.23",
		"@types/node": "^12.12.20",
		"@types/react": "^16.9.16",
		"@types/react-dom": "^16.9.4",
		"@types/three": "^0.103.2",
		"bootstrap": "^4.4.1",
		"jest-config": "^25.1.0",
		"jsdoc": "^3.6.5",
		"open-simplex-noise": "^2.2.0",
		"react": "^16.12.0",
		"react-bootstrap": "^1.0.0-beta.16",
		"react-dom": "^16.12.0",
		"simplex-noise": "^2.4.0",
		"three": "^0.110.0",
		"ts-jest": "^26.3.0"
	},
	"devDependencies": {
		"css-loader": "^4.2.1",
		"electron": "^7.1.5",
		"style-loader": "^1.2.1",
		"typescript": "^3.7.3",
		"webpack": "^4.41.3",
		"webpack-cli": "^3.3.10"
	},
	"jest": {
		"coverageReporters": [
			"json-summary",
			"text",
			"lcov"
		]
	}
}
